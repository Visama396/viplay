{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/components": [
        "src/components/*"
      ],
      "@/icons": [
        "src/icons/*"
      ],
      "@/lib": [
        "src/lib/*"
      ],
      "@/store": [
        "src/store/*"
      ],
      "@/services": [
        "src/services/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}